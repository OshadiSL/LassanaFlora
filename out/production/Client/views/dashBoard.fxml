            </ImageView>
         </graphic>
         <font>
            <Font name="Algerian" size="21.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
      <JFXButton fx:id="btnChef" alignment="CENTER" contentDisplay="TOP" layoutX="391.0" layoutY="119.0" onAction="#chefOrderOnAction" prefHeight="150.0" prefWidth="150.0" styleClass="menue-Button" stylesheets="@../css/Dashboard.css" text="Florist" textFill="#1f036b">
         <graphic>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icons8_Employee_50px.png" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font name="Algerian" size="21.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
   </children>
</AnchorPane>
