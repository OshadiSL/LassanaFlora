            <Font name="Algerian" size="37.0" />
         </font>
      </Label>
      <JFXButton fx:id="btnAdmin" alignment="CENTER" contentDisplay="TOP" layoutX="42.0" layoutY="118.0" onAction="#adminOnAction" prefHeight="150.0" prefWidth="150.0" styleClass="menue-Button" stylesheets="@../css/Dashboard.css" text="ADMIN" textFill="#16035e">
         <graphic>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icons8_Customer_48px.png" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font name="Algerian" size="25.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
      <JFXButton fx:id="btnReception" alignment="CENTER" contentDisplay="TOP" layoutX="215.0" layoutY="118.0" onAction="#receptionOnAction" prefHeight="150.0" prefWidth="150.0" styleClass="menue-Button" stylesheets="@../css/Dashboard.css" text="RECEPTION" textFill="#1f036b">
