<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.dashBordController">
   <children>
      <ImageView fitHeight="79.0" fitWidth="197.0" layoutY="-2.0" pickOnBounds="true">
         <image>
            <Image url="@../images/logo.gif" />
         </image>
      </ImageView>
      <Label alignment="CENTER" opacity="0.71" prefHeight="60.0" prefWidth="604.0" style="-fx-background-color: #1a237e;" text="Dash Board" textFill="WHITE">
